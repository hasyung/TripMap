<%= simple_form_for([:admin,@county]) do |f| %>
	<%= f.input :name %>
	<%= f.input :slug %>
	<%= f.association :city, collection: City.all, prompt: t("helpers.links.select", :model_name => City.model_name.human) %>
	<div class="form-actions">
		<%= f.button :submit, class: 'btn-primary' %>
	</div>
<% end %>